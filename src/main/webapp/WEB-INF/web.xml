<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <!-- Mapping for the admin/manageTests.jsp page -->
  <servlet>
    <servlet-name>ManageTestsServlet</servlet-name>
    <servlet-class>com.example.ManageTestsServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ManageTestsServlet</servlet-name>
    <url-pattern>/quizify/admin/tests</url-pattern>
  </servlet-mapping>
</web-app>
